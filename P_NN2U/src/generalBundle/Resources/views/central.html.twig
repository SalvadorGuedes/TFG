{# app/Resources/views/general/index.html.twig #}
{% set  imagen = 'uploads/images/' ~ ruta_imagen %}
<div class="modal-wrapper" id="popup">
    <div class="popup-contenedor" id="modal"></div>
</div>
<a href="#popup" class="popup-link" id="miEnlace"></a>
<div>
    <div id="cargando" style="width: 100%; height: 100%; text-align: center">
        <li class="loading">
            <p class="loading-bar"></p>
            <p class="loading-bar"></p>
            <p class="loading-bar"></p>
            <p class="loading-bar"></p>
        </li>
    </div>
    <img class="imagen" src="{{ asset(imagen) }}" />
</div>

<div class="lista_botones">
    {{ render(controller('generalBundle:general:form')) }}
    <button class="button" id="llamada"><span> LLAMADA </span></button>
</div>

